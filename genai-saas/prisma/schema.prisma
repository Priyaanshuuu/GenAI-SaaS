
generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User{
  id String
  email String @unique
  name String?
  password String
  image Stirng
  plan String @default("FREE")
  summaries Summary[]
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}


